        android:pathData="M37 72 L54 34"
        android:strokeWidth="7"
        android:strokeLineCap="round">
      <aapt:attr name="android:strokeColor">
        <gradient
            android:startX="37"
            android:startY="72"
            android:endX="54"
            android:endY="34"
            android:type="linear">
          <item android:color="#4285F4" android:offset="0.0"/>
          <item android:color="#EA4335" android:offset="1.0"/>
        </gradient>
      </aapt:attr>
    </path>

    <path
        android:pathData="M71 72 L54 34"
        android:strokeWidth="7"
        android:strokeLineCap="round">
