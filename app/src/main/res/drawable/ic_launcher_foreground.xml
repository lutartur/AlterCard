            android:startY="72"
            android:endX="54"
            android:endY="34"
            android:type="linear">
          <item android:color="#FBBC05" android:offset="0.0"/>
          <item android:color="#EA4335" android:offset="1.0"/>
        </gradient>
      </aapt:attr>
    </path>

    <path
        android:pathData="M44.5 59 L63.5 59"
        android:strokeWidth="7"
        android:strokeLineCap="round">
      <aapt:attr name="android:strokeColor">
        <gradient
            android:startX="44.5"
